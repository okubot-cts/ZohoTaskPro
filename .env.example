# Zoho CRM API Configuration
# Copy this file to .env and fill in your actual values

VITE_ZOHO_CLIENT_ID=your_zoho_client_id_here
ZOHO_CLIENT_SECRET=your_zoho_client_secret_here
VITE_ZOHO_REDIRECT_URI=http://localhost:3001/auth/callback

# API Endpoints (usually don't need to change these)
VITE_ZOHO_AUTH_URL=https://accounts.zoho.com/oauth/v2/auth
VITE_ZOHO_TOKEN_URL=https://accounts.zoho.com/oauth/v2/token
VITE_ZOHO_CRM_API_URL=https://www.zohoapis.com/crm/v2

# Environment
NODE_ENV=development